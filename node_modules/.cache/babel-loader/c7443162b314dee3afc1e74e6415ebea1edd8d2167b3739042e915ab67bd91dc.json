Editing(field) {\n            if (field === 'name') {\n                this.editingName = false;\n            } else if (field === 'email') {\n                this.editingEmail = false;\n            } else if (field === 'phone') {\n                this.editingPhone = false;\n            } else if (field === 'address') {\n                this.editingAddress = false;\n            }\n        },\n        moreInfo(movieID){\n                this.$emit(\"selected\", movieID);\n            }\n    }\n  };\n  </script>\n  \n  <style>\n\n    #welcome{\n        font-size: 40px;\n        margin: 4rem;\n        }\n    #message{\n        font-size: 30px;\n        margin: 4rem;\n    }\n    #information_header{\n        font-size: 30px;\n    }\n    #information{\n        font-size: 20px;\n        margin-top: 4rem;\n        margin-left: 3rem;\n        margin-bottom: 5rem;\n    }\n    #edit_button{\n        margin-right: 10rem;\n    }\n    #save_button{\n        margin-left: 5rem;\n    }\n    #cancel_button{\n        \n        margin-left: 1rem;\n    }\n    #info_labels{\n        margin-right: 1rem;\n    }\n    #movie_list_header{\n        margin-top: 10rem;\n        margin-left: 4rem;\n        font-size: 30px;\n    }\n    #movie_list{\n        margin-left: 3rem;\n        margin-right: 4rem;\n        margin-bottom: 4rem;\n        margin-top: 4rem;\n    }\n    #poster_border{\n        margin-right: 4rem;\n    }\n    #poster{\n        height: 20rem;\n        width: 15rem;\n        border: 4px solid rgb(50, 134, 230);\n    }\n    #poster:hover{\n        border: 4px solid rgb(255, 255, 255);\n    }\n\n  </style>\n  "],"mappings":";AAsEE,eAAe;EACbA,IAAI,EAAE,aAAa;EACnBC,IAAIA,CAAA,EAAG;IACH,OAAO;MACHC,QAAQ,EAAE,MAAM;MAChBC,QAAQ,EAAE,MAAM;MAChBC,YAAY,EAAE,EAAE;MAChBC,WAAW,EAAE,KAAK;MAClBC,KAAK,EAAE,gBAAgB;MACvBC,SAAS,EAAE,EAAE;MACbC,YAAY,EAAE,KAAK;MACnBC,KAAK,EAAE,cAAc;MACrBC,SAAS,EAAE,EAAE;MACbC,YAAY,EAAE,KAAK;MACnBC,OAAO,EAAE,YAAY;MACrBC,WAAW,EAAE,EAAE;MACfC,cAAc,EAAE,KAAK;MACrBC,SAAS,EA